<h1>投稿フォーム</h1>
<%= form_with model: @toukou,lacal:true do |f| %>
　<h4>マップ</h4>
　<%= f.select :map,[["アセント",1],["バインド",2],["ブリーズ",3],["フラクチャー",4],["ヘイブン",5],["アイスボックス",6],["ロータス",7],["パール",8],["スプリット",9]],include_blank: "マップを選択してください" %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>立ち位置</h4>
  <%= f.attachment_field :location %>
  <h4>目印</h4>
  <%= f.attachment_field :point %>
  <h4>結果</h4>
  <%= f.attachment_field :result %>
  <h4>説明</h4>
  <%= f.text_area :explain %>
  <%= f.submit "投稿" %>
<% end %>